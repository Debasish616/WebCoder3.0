{
    "name": "datavault",
    "description": "datavault description",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html",
      "default_title": "Datavault"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "assets/logo/logo192.png",
      "48": "assets/logo/logo192.png",
      "128": "assets/logo/logo192.png"
    },
    "permissions": [
      "activeTab",
      "tabs",
      "webNavigation",
      "storage"
    ],
    "optional_permissions": []
  }

  